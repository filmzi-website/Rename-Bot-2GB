{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/webhook.py"
    }
  ],
  "functions": {
    "api/webhook.py": {
      "maxDuration": 30
    }
  }
}
