{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/health",
      "dest": "/api/webhook.py"
    }
  ]
}
